<div class="form">
    <%= form_with url: landing_track_shipment_path do |f| %>
        <div class="form-group row">
            <div class="col-sm-10">
                <%= f.text_field :shipment_code, placeholder: 'SP0000', class: "form-control" %>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-4 col-sm-offset-2">
                <%= submit_tag "Search shipment", class: "btn btn-primary" %>
            </div>
        </div>
    <% end %>
</div>
